const int mod           = 1e9 + 7;
const int inf           = (int)2e9 + 5;
const ll  Inf           = (ll)1e18 + 5;
const int N             = 1e7 + 5;
const int nn           = 3e5 + 5;

int siv[2000];

int solve()
{
	//happy coding
	int a,b,p;
	cin>>a>>b>>p;
	string s;
	cin>>s;
	int n = s.size();
	for(int i=n-2;i>=0;i--){
		//cout<<p<<" ";
		if(i == n-2){
			if(s[n-2]=='A'&&a<=p) p = p-a;
			else if(s[n-2]=='B'&&b<=p){
			 p = p - b;//cout<<"Ok"<<endl;
			}
			else{
				cout<<n<<endl;
				//cout<<"Ok"<<endl;
				return 0;
			}
			if(s[i]!=s[i-1]){
				if(s[i-1]=='A'&&a<=p){
					p -= a;
				}else if(s[i-1]=='B'&&b<=p){
					p-=b;
				}else{
					cout<<i+1<<endl;
					//cout<<"ok"<<endl;
					return 0;
				}
			}
		}else {
			if(s[i]!=s[i-1]){
				if(s[i-1]=='A'&&a<=p){
					p -= a;
				}else if(s[i-1]=='B'&&b<=p){
					p-=b;
				}else{
					cout<<i+1<<endl;
					//cout<<"ok"<<endl;
					return 0;
				}
			}
		}
	}


	//error();
	return 0;
}
 
int main()
{
    int test = 1, tc = 0;
    scanf("%d", &test);//(void)getchar();
    while (test--)solve();return 0;
}
