#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long x1,y1,x2,y2;
    cin>>x1>>y1>>x2>>y2;
    if((x1*x1+y1*y1)<(x2*x2+y2*y2)){
        cout<<"Russo"<<endl;
    } else if((x1*x1+y1*y1)>(x2*x2+y2*y2)){
        cout<<"Wil"<<endl;
    }
    else cout<<"Empate"<<endl;
    return 0;
 
}
